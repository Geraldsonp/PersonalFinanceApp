@page "/transactions"
@using PersonalFinanceApp.Components
<Card PageTitle="Transactions">
    <TransactionList transactions=@TransactionsList></TransactionList>
</Card>
@code {
    public IEnumerable<Transaction> TransactionsList { get; set; } = new List<Transaction>();
    [Inject] ITransactionService? transactionService { get; set; }

    protected override async Task OnInitializedAsync()
    {
        TransactionsList = await transactionService?.GetAsync();
    }
}